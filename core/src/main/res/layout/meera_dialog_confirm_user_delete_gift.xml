<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical">

    <com.meera.uikit.widgets.cell.UiKitCell
        android:id="@+id/v_delete_gift"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cellLeftElement="left_icon"
        app:cellLeftIconSrc="@drawable/ic_outlined_delete_m"
        tools:cellCityValue=""
        app:cellTitleValue="@string/meera_gifts_delete"
        app:cellLeftIconAndTitleColor="@color/ui_red"
        android:layout_marginHorizontal="16dp"
        android:layout_marginBottom="8dp"
        app:cellBackgroundColor="@color/uiKitColorBackgroundSecondary"/>
</LinearLayout>
