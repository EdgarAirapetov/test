            <enum name="SIZE_70" value="7" />
            <enum name="SIZE_60" value="3" />
            <enum name="SIZE_48" value="6" />
            <enum name="SIZE_45" value="4" />
            <enum name="SIZE_41" value="5" />
            <enum name="SIZE_35" value="8" />
