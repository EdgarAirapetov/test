    <declare-styleable name="ScalableImageView">
        <!-- Scale factor for image enlarging -->
        <attr name="scale" format="float" />
        <!-- Coordinates for pivot point used for scaling -->
        <attr name="scalePivotX" format="fraction" />
        <attr name="scalePivotY" format="fraction" />
        <!-- Max scale factor -->
        <attr name="maxScale" format="float" />
    </declare-styleable>

