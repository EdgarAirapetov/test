<?xml version="1.0" encoding="utf-8"?><!--<LinearLayout-->
<com.facebook.shimmer.ShimmerFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/vg_cell_shimmer"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginHorizontal="16dp"
    android:layout_marginVertical="6dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/radius_12_shimmer"
        android:backgroundTint="@color/uiKitColorBackgroundSecondary">

        <View
            android:id="@+id/v_image_shimmer"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_margin="8dp"
            android:background="@drawable/circle_shimmer"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/v_image_vehicle_shimmer"
            android:layout_width="122dp"
            android:layout_height="142dp"
            android:background="@drawable/rect_rad_start_12"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <View
            android:id="@+id/v_first_line_shimmer"
            android:layout_width="100dp"
            android:layout_height="8dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="18dp"
            android:background="@drawable/line_shimmer"
            app:layout_constraintStart_toEndOf="@id/v_image_vehicle_shimmer"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/v_second_line_shimmer"
            android:layout_width="100dp"
            android:layout_height="8dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            android:background="@drawable/line_shimmer"
            app:layout_constraintStart_toEndOf="@id/v_image_vehicle_shimmer"
            app:layout_constraintTop_toBottomOf="@+id/v_first_line_shimmer" />

        <View
            android:id="@+id/v_third_line_shimmer"
            android:layout_width="120dp"
            android:layout_height="30dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/line_shimmer"
            app:layout_constraintStart_toEndOf="@id/v_image_vehicle_shimmer"
            app:layout_constraintTop_toBottomOf="@+id/v_second_line_shimmer" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</com.facebook.shimmer.ShimmerFrameLayout>

